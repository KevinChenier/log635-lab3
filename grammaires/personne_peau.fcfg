%start S

S[SEM=<PeauVerte(?vp)>] -> VP[SEM=?vp]
VP[SEM=?n] -> N[SEM=?n] V Det Npeau AD
N[SEM=?np] -> Nprop[SEM=?np]
V -> 'a'
Det -> 'la'
AD -> 'verte'
Npeau -> 'peau'

Nprop[SEM=<Rouge>] -> 'Rouge'
Nprop[SEM=<Mauve>] -> 'Mauve'
Nprop[SEM=<Bleu>] -> 'Bleu'
Nprop[SEM=<Vert>] -> 'Vert'
Nprop[SEM=<Jaune>] -> 'Jaune'
Nprop[SEM=<Blanc>] -> 'Blanc'
Nprop[SEM=<Violet>] -> 'Violet'
Nprop[SEM=<Turquoise>] -> 'Turquoise'
Nprop[SEM=<Noir>] -> 'Noir'
Nprop[SEM=<Rose>] -> 'Rose'