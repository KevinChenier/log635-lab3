     "   \"cell_type\": \"code\",\n",
     "   \"execution_count\": 52,\n",
     "   \"metadata\": {},\n",
     "   \"outputs\": [\n",
     "    {\n",
     "     \"data\": {\n",
     "      \"text/plain\": [\n",
     "       \"{A: True, B: True}\"\n",
     "      ]\n",
     "     },\n",
     "     \"execution_count\": 52,\n",
     "     \"metadata\": {},\n",
     "     \"output_type\": \"execute_result\"\n",
     "    }\n",
     "   ],\n",
     "   \"source\": [\n",
     "    \"dpll_satisfiable(A |'<=>'| B)\"\n",
     "   ]\n",
