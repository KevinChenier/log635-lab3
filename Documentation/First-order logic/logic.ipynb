     "   },\n",
     "   \"outputs\": [],\n",
     "   \"source\": [\n",
     "    \"definite_clauses_KB = PropDefiniteKB()\\n\",\n",
     "    \"for clause in clauses:\\n\",\n",
     "    \"    definite_clauses_KB.tell(expr(clause))\"\n",
     "   ]\n",
     "  },\n",
