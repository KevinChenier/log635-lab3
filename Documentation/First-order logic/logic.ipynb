     "   \"outputs\": [],\n",
     "   \"source\": [\n",
     "    \"A, B, C, D = expr('A, B, C, D')\"\n",
     "   ]\n",
     "  },\n",
     "  {\n",
     "   \"cell_type\": \"code\",\n",
     "   \"execution_count\": 50,\n",
     "   \"metadata\": {},\n",
     "   \"outputs\": [\n",
     "    {\n",
     "     \"data\": {\n",
