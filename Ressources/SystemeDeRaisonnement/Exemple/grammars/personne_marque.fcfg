%start S

S[SEM=<MarqueCou(?vp)>] -> VP[SEM=?vp]
VP[SEM=?n] -> N[SEM=?n] V Det Nmarque P Ncou
N[SEM=?np] -> Nprop[SEM=?np]
V -> 'a'
Det -> 'des' | 'une'
P -> 'au'
AD -> 'mort' | 'morte'
Nmarque -> 'marques' | 'marque'
Ncou -> 'cou'

Nprop[SEM=<Rouge>] -> 'Rouge'
Nprop[SEM=<Mauve>] -> 'Mauve'
Nprop[SEM=<Bleu>] -> 'Bleu'
Nprop[SEM=<Vert>] -> 'Vert'
Nprop[SEM=<Jaune>] -> 'Jaune'
Nprop[SEM=<Blanc>] -> 'Blanc'
