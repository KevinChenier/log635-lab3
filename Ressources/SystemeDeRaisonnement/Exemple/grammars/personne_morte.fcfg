%start S

S[SEM=<EstMort(?vp)>] -> VP[SEM=?vp]
VP[SEM=?n] -> N[SEM=?n] V AD
N[SEM=?np] -> Nprop[SEM=?np]
V -> 'est'
AD -> 'mort' | 'morte'

Nprop[SEM=<Rouge>] -> 'Rouge'
Nprop[SEM=<Mauve>] -> 'Mauve'
Nprop[SEM=<Bleu>] -> 'Bleu'
Nprop[SEM=<Vert>] -> 'Vert'
Nprop[SEM=<Jaune>] -> 'Jaune'
Nprop[SEM=<Blanc>] -> 'Blanc'